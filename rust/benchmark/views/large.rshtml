<html>
    <head>
        larg size of content
    </head>
    <body>
        <div class="title">
           Large
        </div>
        <div class="content">
           <table>
              @{
                  let size = 100;
                  for row in 0..size {
                      @{<tr>}
                      for col in 0..size {
                        @{<td>@row @col</td>}
                      }
                      @{<tr>}
                  }
              }
           </table>
        </div>
    </body>
</html>