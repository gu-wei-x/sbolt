@{
    let name = context.get_data::<String>("name").clone();
    let age = context.get_data::<i32>("age").clone();
    let msg = context.get_data::<String>("msg").clone();
}
<html>
    <head>
        <title>Index</title>
    </head>
    <body>
        <div>@name(@age) says: @msg</div>
    </body>
</html>